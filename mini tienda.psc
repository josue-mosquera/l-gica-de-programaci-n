Proceso Maniana
	Definir N, i Como Entero;
	Definir monto, precio Como Real;
	Escribir "Cantidad de articulos:";
	Leer N;
	monto <- 0
	Escribir "Precios:"
	
	Para i<-1 Hasta N  Hacer;
		Escribir " ingrese el precio del producto ", i
		leer precio 
		monto <- precio + monto
		
	Fin Para
	/// complete aquí el pseudocódigo faltante
	/// que debe solicitar los N precios y 
	/// calcular el monto final a pagar
	
	
	Escribir "Monto a pagar: $ ", Monto;
	
FinProceso
